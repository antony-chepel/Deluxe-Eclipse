<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".rwerasd.Kwqoqyuxgctftfxcs">

    <ImageView
        android:id="@+id/fokeko"
        android:layout_width="250dp"
        android:layout_height="150dp"
        android:layout_marginTop="20dp"
        android:elevation="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/del_ecl_2" />

    <TableLayout
        android:id="@+id/tableLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/back_del"
        android:gravity="center"
        android:padding="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:context=".blackneon.MainActivity">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_marginTop="20dp"
            android:gravity="center">

            <TextView
                android:id="@+id/imageView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Find all deluxe rings and get bonus"
                android:textColor="@color/white"
                android:fontFamily="@font/del_ecl_b"
                android:textSize="20sp"
                android:textStyle="bold" />
        </LinearLayout>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:gravity="center">

            <ImageView
                android:id="@+id/button"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_marginLeft="5dp"
                android:background="#FFFFFF"
                android:backgroundTint="@color/prm_clr"
                android:onClick="tdyshuwjisd"
                android:padding="8dp" />

            <ImageView
                android:id="@+id/button2"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_marginLeft="5dp"
                android:background="#FFFFFF"
                android:backgroundTint="@color/prm_clr"
                android:onClick="tdyshuwjisd"
                android:padding="8dp"
                android:textSize="60dp" />

            <ImageView
                android:id="@+id/button3"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_marginLeft="5dp"
                android:background="#FFFFFF"
                android:backgroundTint="@color/prm_clr"
                android:onClick="tdyshuwjisd"
                android:padding="8dp"
                android:textColor="#000000"
                android:textSize="60dp" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center">

            <ImageView
                android:id="@+id/button4"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="5dp"
                android:background="#FFFFFF"
                android:backgroundTint="@color/prm_clr"
                android:onClick="tdyshuwjisd"
                android:padding="8dp"
                android:textColor="#000000"
                android:textSize="60dp" />

            <ImageView
                android:id="@+id/button5"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="5dp"
                android:background="#FFFFFF"
                android:backgroundTint="@color/prm_clr"
                android:onClick="tdyshuwjisd"
                android:padding="8dp"
                android:textColor="#000000"
                android:textSize="60dp" />

            <ImageView
                android:id="@+id/button6"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="5dp"
                android:background="#FFFFFF"
                android:backgroundTint="@color/prm_clr"
                android:onClick="tdyshuwjisd"
                android:padding="8dp"
                android:textColor="#000000"
                android:textSize="60dp" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center">

            <ImageView
                android:id="@+id/button7"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="5dp"
                android:background="#FFFFFF"
                android:backgroundTint="@color/prm_clr"
                android:onClick="tdyshuwjisd"
                android:padding="8dp"
                android:textColor="#000000"
                android:textSize="60dp" />

            <ImageView
                android:id="@+id/button8"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="5dp"
                android:background="#FFFFFF"
                android:backgroundTint="@color/prm_clr"
                android:onClick="tdyshuwjisd"
                android:padding="8dp"
                android:textColor="#000000"
                android:textSize="60dp" />

            <ImageView
                android:id="@+id/button9"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="5dp"
                android:background="#FFFFFF"
                android:backgroundTint="@color/prm_clr"
                android:onClick="tdyshuwjisd"
                android:padding="8dp"
                android:textColor="#000000"
                android:textSize="60dp" />

        </TableRow>

    </TableLayout>

    <TextView
        android:id="@+id/tv_wld_score"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Deluxe bonus : 0"
        android:textColor="@color/white"
        android:fontFamily="@font/del_ecl_ex_bld"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fokeko"
        app:layout_constraintVertical_bias="0.89" />

</androidx.constraintlayout.widget.ConstraintLayout>